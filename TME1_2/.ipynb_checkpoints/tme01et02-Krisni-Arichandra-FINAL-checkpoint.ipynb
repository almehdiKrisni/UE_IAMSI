   "source": [
    "# Méthode permettant de savoir si le joueur ayant la main peut jouer sur une case précise\n",
    "# Case de son côté et ayant au moins 1 graine\n",
    "def est_correct(position, nombre) :\n",
    "    # On vérifie si n est compris entre 1 et n\n",
    "    if (nombre < 1) or (nombre > position[\"dimension\"]) :\n",
    "        return False\n",
    "    \n",
    "    # On récupère la position du joueur ayant la main (Sud ou Nord)\n",
    "    if position[\"trait\"] == \"NORD\" :\n",
    "        case = position[\"dimension\"] * 2 - nombre\n",
    "    else :\n",
    "        case = nombre - 1\n",
    "            \n",
    "    # On vérifie si la case contient au moins une graine\n",
    "    if position[\"plateau\"][case] >= 1 :\n",
    "        return True\n",
    "    return False\n",
    "\n",
    "# Méthode permettant de savoir si le joueur devant jouer possède au moins une graine dans son camp\n",
    "def est_legale(position) :\n",
    "    # On vérifie quel est le joueur ayant la main\n",
    "    main = 0\n",
    "    if (position[\"trait\"] == \"NORD\") :\n",
    "        main = 1\n",
    "    \n",
    "    # On parcourt toutes les cases du joueur et on vérifie si il possède au moins 1 graine dans son camp\n",
    "    for i in range(position[\"dimension\"] * main, position[\"dimension\"] * (main + 1)) :\n",
    "        if (position[\"plateau\"][i] >= 1) :\n",
    "            return True\n",
    "    return False\n",
    "\n",
    "# Méthode permettant de jouer un coup si le coup est correct et si la position résultante est légale\n",
    "def effectue_si_valide(position, coup) :\n",
    "    # On vérifie si le coup est correct (méthode est_correct)\n",
    "    if not(est_correct(position, coup)) :\n",
    "        return False\n",
    "    \n",
    "    # On joue le coup\n",
    "    new_pos = joue_un_coup(position, coup)\n",
    "    \n",
    "    # On vérifie si la nouvelle position est légale\n",
    "    return (est_legale(new_pos))\n",
    "\n",
    "# Méthode permettant de savoir si la position est terminale \n",
    "def est_terminale(position) :\n",
    "    # On vérifie si un des joueurs a au moins 25 graines dans son butin\n",
    "    for value in position[\"butin\"].values() :\n",
    "        if (value >= position[\"dimension\"] * 4 + 1) :\n",
    "            return True\n",
    "        \n",
    "    # On vérifie si pour chaque coup, qu'il soit correct ou pas, et pour chaque position résultant d'un \n",
    "    # coup correct, la position est terminale\n",
    "    for coup in range(1, position[\"dimension\"]) :\n",
    "        if (effectue_si_valide(position, coup)) :\n",
    "            return False\n",
    "    return True"
   ]
